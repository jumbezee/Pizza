<h2>Welcome to your CART</h2>


<table class="table table-striped"> 
	<thead> 
		<tr> 
			<th>#</th> 
			<th>Title</th> 
			<th>Price</th> 
			<th>Qty</th> 
		</tr> 
	</thead> 
	<tbody>

	<% @items.each_with_index do |item, i| %>
		<tr> 
			<th scope="row"><%=i + 1%></th> 
				<td><%= item[0].title%></td> 
				<td><%= item[0].price%></td> 
				<td><%= item[1]%></td> 
		</tr> 
	<% end %>
	</tbody> 
</table>